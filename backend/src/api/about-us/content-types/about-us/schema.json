{
  "kind": "singleType",
  "collectionName": "about_uses",
  "info": {
    "singularName": "about-us",
    "pluralName": "about-uses",
    "displayName": "About Us",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "body": {
      "type": "blocks"
    },
    "title": {
      "type": "string"
    },
    "subtitle": {
      "type": "string"
    },
    "body_caption": {
      "type": "string"
    },
    "teams": {
      "type": "component",
      "repeatable": true,
      "component": "shared.team"
    },
    "issues": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::issue.issue"
    },
    "credit_tagline": {
      "type": "text"
    },
    "fonts": {
      "type": "blocks"
    },
    "copyright_tag": {
      "type": "text"
    },
    "alt_color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    }
  }
}
